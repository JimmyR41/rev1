<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>New Article</title>
  </head>
  <body>
    <%= form_for @article do |f|%>
      <%= f.label :title%>
      <%= f.text_field :title%>
<br>
      <%= f.label :description%>
      <%= f.text_field :description%>
<br>
      <%= f.submit%>





    <% end %>




  </body>
</html>
